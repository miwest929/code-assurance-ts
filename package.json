{
  "name": "code-assurance",
  "version": "0.0.1",
  "description": "Command line tool for analyzing the code quality of your project through ABC metrics",
  "author": "Michael West",
  "keywords": [
    "typescript",
    "cli",
    "abc",
    "quality"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/assurance.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/assurance.ts",
    "build": "npm run clean && tsc",
    "test": "npm run build && ava --verbose",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "commander": "^2.20.3",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3",
    "@types/node": "^13.9.0"
  },
  "dependencies": {}
}
